<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" autoDrawBackground="false" 
				>
	<s:states>
		<s:State name="normal" />
		<s:State name="hovered" />
		<s:State name="selected" />
	</s:states>
	<s:BitmapImage includeIn="selected" width="100%" height="100%" source="@Embed('/embed/ui/select.png',scaleGridTop='8',scaleGridBottom='16',scaleGridLeft='31',scaleGridRight='400')"/>
	<s:BitmapImage includeIn="hovered" width="100%" height="100%" source="@Embed('/embed/ui/over.png',scaleGridTop='8',scaleGridBottom='16',scaleGridLeft='31',scaleGridRight='400')"/>
</s:ItemRenderer>